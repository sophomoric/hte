<!DOCTYPE html>
<html>
<head>
  <title>Hte</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>

  <% if flash[:message] %>
    <%= flash[:message] %>
  <% end %>
  <%= flash[:errors] %>

  <% if current_user %>
    <%= render partial: "layouts/menu" %>
  <% else %>
    <%= link_to "login", new_user_session_url %>
  <% end %>

  <%= yield %>

</body>
</html>
